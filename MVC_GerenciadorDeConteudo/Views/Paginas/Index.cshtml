
@{
    ViewBag.Title = "Paginas";
    List<Business.Pagina> paginas = ViewBag.Paginas;
}


<div class="jumbotron">
    <h1>Páginas</h1>
    <table class="table">
        <tr>
            <td>Id</td>
            <td>Nome</td>
            <td>Data</td>
        </tr>
        @foreach (var p in paginas)
        {
            <tr>
            <td>@p.Id:</td>
            <td>@p.Name:</td>
            <td>@p.Data:</td>
            </tr>
        }
    </table>
</div>